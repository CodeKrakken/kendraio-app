{
	"title": "PRS recording",
	"description": "Register a recording with PRS.",
	"type": "object",
	"required": [
		"isrc",
		"bandArtist",
		"recordingTitle",
		"contentType",
		"pDate"
	],
	"properties": {
		"isrc": {
			"type": "string",
			"title": "ISRC",
			"minLength": 12,
			"maxLength": 12,
			"default": ""
		},
		"bandArtist": {
			"type": "string",
			"title": "Band/Artist name"
		},
		"recordingTitle": {
			"type": "string",
			"title": "Recording Title"
		},
		"versionType": {
			"type": "string",
			"title": "Version Type",
			"enum": [
				"Album",
				"Alternative",
				"Demo",
				"Edit",
				"Instrumental",
				"Karaoke",
				"Live",
				"Radio edit",
				"Remix",
				"Session"
			]
		},
		"remastered": {
			"type": "boolean",
			"title": "Remastered",
			"description": "This recording is remastered"
		},
		"genre": {
           "type": "string",
           "title":"Genre",
			"enum": [
				"Alternative Rock",
				"Adult contemporary",
				"Bhangra",
				"Blues",
				"Chamber music",
				"Children's music",
				"Christian",
				"Classical",
				"Country",
				"Dance",
				"Easy listening",
				"Erotic",
				"Folk",
				"Gospel",
				"Hip Hop",
				"Jazz",
				"Latin",
				"Middle of the road",
				"Musical (theatre)",
				"New Age",
				"Opera",
				"Operetta",
				"Other",
				"Pop",
				"Rap",
				"Reggae",
				"Religious",
				"Rhythm and Blues",
				"Rock",
				"Rock and Roll",
				"Show",
				"Soul",
				"Sound effects",
				"Spoken word",
				"Unclassified",
				"World music"
			]
		},
		"contentType": {
			"type": "string",
			"title": "Content Type",
			"enum": [
				"Audio",
				"video"
			]
		},
		"pDate": {
			"type": "datepicker",
			"title": "(P)Date ",
			"minLength": 4,
			"maxLength": 4

		}
	}
}
