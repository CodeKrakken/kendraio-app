{
  "version": 2,
  "alias": [
    "dev.app.kendra.io"
  ],
  "github": {
    "enabled": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "useBuildUtils": "@now/build-utils@canary"
      }
    }
  ],
  "routes": [
    {
      "src": "^/3rdpartylicense.txt",
      "dest": "/kendraio-app/3rdpartylicense.txt"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/kendraio-app/favicon.ico"
    },
    {
      "src": "^/main(.*)",
      "dest": "/kendraio-app/main$1"
    },
    {
      "src": "^/polyfills(.*)",
      "dest": "/kendraio-app/polyfills$1"
    },
    {
      "src": "^/styles(.*)",
      "dest": "/kendraio-app/styles$1"
    },
    {
      "src": "^/runtime(.*)",
      "dest": "/kendraio-app/runtime$1"
    },
    {
      "src": "^/(.*).js$",
      "dest": "/kendraio-app/$1.js"
    },
    {
      "src": "^/(.*).json$",
      "dest": "/kendraio-app/$1.json"
    },
    {
      "src": "^/(.*).yaml$",
      "dest": "/kendraio-app/$1.yaml"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/kendraio-app/assets/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/kendraio-app/index.html"
    }
  ]
}
